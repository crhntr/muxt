package server

import (
	"embed"
	"html/template"
)

var (
	//go:embed *.gohtml
	formHTML embed.FS

	//go:generate go run github.com/crhntr/muxt/cmd/muxt
	templates = template.Must(template.ParseFS(formHTML, "*"))
)