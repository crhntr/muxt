-- template_test.go --
package server

import (
	"testing"

	"net/http"
	"net/http/httptest"
)

func Test(t *testing.T) {
	mux := http.NewServeMux()

	TemplateRoutes(mux, nil)

	req := httptest.NewRequest(http.MethodGet, "/end/", nil)
	rec := httptest.NewRecorder()

	mux.ServeHTTP(rec, req)

	res := rec.Result()

	if res.StatusCode != http.StatusOK {
		t.Error("expected OK")
	}
}
