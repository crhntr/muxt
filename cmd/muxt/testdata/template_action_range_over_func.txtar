package server

import (
	"embed"
	"html/template"
	"iter"
)

//go:embed *.gohtml
var formHTML embed.FS

var templates = template.Must(template.ParseFS(formHTML, "*"))

type T struct{}

func (T) Iterator() iter.Seq2[int, float64] {
	return func(yield func(int, float64) bool) {
		for i := range 5 {
			if !yield(i+1, float64((i+1)*(i+1))) {
				return
			}
		}
	}
}
